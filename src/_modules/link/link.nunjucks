{% macro content(spec) %}

{% set spec = spec or {} %}
{% set _id = spec.id or '' %}
{% set _class = spec.class or '' %}
{% set _href = spec.href or '#' %}
{% set _target = spec.target or '_self' %}
{% set _text = spec.text or 'Default Link' %}

{% set _icon = spec.icon or '' %}

{# <a
  id="{{_id}}"
  class="link {{_class}}"
  href="{{_href}}"
  target="{{_target}}"
>
  {% if _icon %}
    <span>{{_icon}}</span>
  {% endif %}
  {{_text}}
</a> #}
{% endmacro %}
